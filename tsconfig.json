{
  "compilerOptions": {
      "baseUrl": ".",
      "outDir": "build",
      "module": "commonjs",
      "target": "es2019",
      "esModuleInterop": true,
      "lib": ["es2019", "dom"],
      "sourceMap": true,
      "allowJs": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "rootDir": ".",
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "importHelpers": true,
      "strictNullChecks": true,
      "noUnusedLocals": true,
      "resolveJsonModule": true,
      "downlevelIteration": true,
      "allowSyntheticDefaultImports": true,
  }
}
