{
  "author": "Vladimir Simonovski",
  "name": "selenium-cucumber-tests",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/VladimirSimonovski1/Selenium_Cucumber_Tests"
  },
  "scripts": {
    "cucumber-js": "npx cucumber-js -b -p default",
    "prettier": "npx prettier src/**/*.ts* --write",
    "tests": "npm run cucumber-js ./src/features/*.feature & node HTMLReporter.js",
    "tests:parallel": "npm run cucumber-js --parallel 2 ./src/features/*.feature & node HTMLReporter.js",
    "test": "npx cucumber-js -b -p default --tags @single ./src/features/*.feature & node HTMLReporter.js"
  },
  "dependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "@types/chai": "^5.0.1",
    "@types/ini": "^4.1.1",
    "@types/lodash": "^4.17.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.5",
    "@types/selenium-webdriver": "^4.1.28",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "chromedriver": "^133.0.2",
    "cucumber-console-formatter": "^1.0.0",
    "cucumber-junit": "^1.7.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "gherkin-lint": "^4.2.4",
    "ini": "^5.0.0",
    "log": "^6.3.2",
    "mocha": "^11.1.0",
    "multiple-cucumber-html-reporter": "^3.9.2",
    "npm-check-updates": "^17.1.14",
    "prettier": "^3.5.2",
    "selenium-webdriver": "^4.29.0",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.5",
    "@types/log": "^6.3.3",
    "chai": "^4.3.7",
    "ts-node": "^10.9.2"
  }
}
